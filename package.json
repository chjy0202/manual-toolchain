{
  "private": true,
  "type": "module",
  "name": "react-manual-toolchain",
  "version": "1.0.0",
  "description": "React 매뉴얼 툴체인 구성",
  "scripts": {
    "bundle": "webpack bundle --target=browserslist --entry=./src/main.js --output-path=public --mode=development",
    "build": "webpack bundle --target=browserslist --entry=./src/main.js --output-path=./build --mode=production"
  },
  "keywords": [
    "react",
    "webpack",
    "eslint",
    "prettier",
    "babel",
    "typescript",
    "jest",
    "testing-library",
    "sass",
    "postcss",
    "svgr"
  ],
  "author": {
    "name": "chjy0202",
    "email": "chjy0202@hanyang.ac.kr"
  },
  "license": "ISC",
  "devDependencies": {
    "webpack": "^5.74.0",
    "webpack-cli": "^4.10.0"
  }
}
